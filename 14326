var mainnavarray = new Array();
$('#main-menu li').each(function(){
    mainnavarray.push($(this).find('.get'));
    $(this).mouseover(function(){
       var gotget = $(this).find('.get');
       if(gotget.attr("class")){gotget.show();$(this).find('.mmenu-a').addClass("mmenu-a-over");}
    });
    $(this).mouseout(function(){
        for(i=0;mainnavarray.length>=i;i++){$(mainnavarray[i]).hide();$(this).find('.mmenu-a').removeClass("mmenu-a-over");}
    });
});
